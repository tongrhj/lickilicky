{
  "compilerOptions": {
    "outDir": "./output/",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "module": "CommonJS",
    "target": "ES2015",
    "allowJs": false,
    "noImplicitAny": true,
    "noEmitOnError": true,
    "strictNullChecks": true
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}
