{
  "name": "lickilicky",
  "version": "1.0.0",
  "description": "track f&b partners over time",
  "main": "index.js",
  "author": "Jared Tong <tongrhj@users.noreply.github.com>",
  "license": "MIT",
  "engines": {
    "node": ">=12.16.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tongrhj/lickilicky.git"
  },
  "scripts": {
    "build": "tsc && npm run data && npm run frontend",
    "data": "node ./output/scripts/run.js",
    "frontend": "mkdir -p dist/data && cp -R assets dist/ && cp index.html dist/ && cp now.json dist/ && node ./output/scripts/updateLastUpdatedDate.js",
    "lint": "tsc --noEmit",
    "test": "prettier \"{lib,scripts}/*.ts\" --list-different && npm run lint",
    "format": "prettier \"{lib,scripts}/*.ts\" --write",
    "clean": "rm -rf ./dist ./output"
  },
  "devDependencies": {
    "@types/got": "^9.6.9",
    "got": "^9.6.0",
    "husky": "^1.3.1",
    "moment": "^2.24.0",
    "now": "^16.7.3",
    "prettier": "^2.0.0",
    "pretty-quick": "^1.11.1",
    "query-string": "^6.11.1",
    "replace-in-file": "^3.4.4",
    "tough-cookie": "^3.0.1",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
