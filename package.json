{
  "name": "lickilicky",
  "version": "1.0.0",
  "description": "track f&b partners over time",
  "main": "index.js",
  "author": "Jared Tong <tongrhj@users.noreply.github.com>",
  "license": "MIT",
  "engines": {
    "node": ">=12.16.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tongrhj/lickilicky.git"
  },
  "scripts": {
    "build": "tsc && npm run data && npm run frontend",
    "data": "node ./output/scripts/run.js",
    "frontend": "mkdir -p dist/data && cp -R assets dist/ && cp index.html dist/ && cp now.json dist/ && node ./output/scripts/updateLastUpdatedDate.js",
    "lint": "prettier --check \"{lib,scripts}/*.ts\" && tsc --noEmit",
    "format": "prettier \"{lib,scripts}/*.ts\" --write",
    "clean": "rm -rf ./dist ./output"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.149",
    "@types/tough-cookie": "^4.0.0",
    "got": "^10.7.0",
    "husky": "^4.2.3",
    "moment": "^2.24.0",
    "now": "^17.1.1",
    "prettier": "^2.0.0",
    "pretty-quick": "^2.0.1",
    "query-string": "^6.11.1",
    "replace-in-file": "^5.0.2",
    "tough-cookie": "^4.0.0",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "lodash": "^4.17.15"
  }
}
